<% if page.current? %>
  <span class="inline-flex items-center px-4 pt-4 text-sm font-medium text-indigo-600"><%= page %></span>
<% else %>
  <%= link_to page, url, {remote: remote, rel: page.rel, class: "inline-flex items-center px-4 pt-4 text-sm font-medium text-gray-500 hover:text-gray-700"} %>
<% end %>